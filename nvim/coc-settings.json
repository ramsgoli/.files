{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "filetypes": [
                "go"
            ],
            "rootPatterns": [
                "go.mod",
                ".vim/",
                ".git/",
                ".hg/"
            ]
        }
    },
    "coc.preferences.hoverTarget": "float",
    "javascript.suggestionActions.enabled": false,
    "javascript.validate.enable": false,
    "eslint.autoFixOnSave": true,
    "tsserver.maxTsServerMemory": 8096
}
